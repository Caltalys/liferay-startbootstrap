<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 7.1.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_7_1_0.dtd">

<look-and-feel>
	<compatibility>
		<version>7.1.0+</version>
	</compatibility>
	<theme id="startbootstrap-agency" name="StartBootstrap Agency">
		<template-extension>ftl</template-extension>
		<settings>
			<setting configurable="true" key="wrap-content" type="checkbox" value="false" />
		</settings>
		<layout-templates>
			<custom>
				<layout-template id="masthead_row" name="MastHead Row">
					<template-path>/layoutttpl/custom/masthead_row.tpl</template-path>
					<thumbnail-path>/layoutttpl/custom/masthead_row.png</thumbnail-path>
				</layout-template>
			</custom>
		</layout-templates>
		<portlet-decorator id="barebone" name="Barebone">
			<default-portlet-decorator>true</default-portlet-decorator>
			<portlet-decorator-css-class>portlet-barebone</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="borderless" name="Borderless">
			<portlet-decorator-css-class>portlet-borderless</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="decorate" name="Decorate">
			<portlet-decorator-css-class>portlet-decorate</portlet-decorator-css-class>
		</portlet-decorator>
	</theme>
</look-and-feel>
